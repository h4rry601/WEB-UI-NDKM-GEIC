{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": "off",
    "typescript-config/consistent-casing": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "axe/aria": "off",
    "no-inline-styles": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 26",
    "not safari <= 26"
  ]
}